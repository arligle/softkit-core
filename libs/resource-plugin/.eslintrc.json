{
  "extends": [
    "../../.eslintrc.base.json"
  ],
  "parserOptions": {
    "project": [
      "./libs/resource-plugin/tsconfig.lib.json",
      "./libs/resource-plugin/tsconfig.spec.json"
    ]
  },
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "./package.json",
        "./generators.json"
      ],
      "parser": "jsonc-eslint-parser",
      "rules": {
        "@nx/nx-plugin-checks": "error"
      }
    }
  ]
}
