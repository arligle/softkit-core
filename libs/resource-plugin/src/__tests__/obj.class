obj_properties=()

port=0
name=1

obj.property(){
    if [ "$2" == "=" ]
    then
        obj_properties[$1]=$3
    else
        echo ${obj_properties[$1]}
    fi
}

obj.port(){
    if [ "$1" == "=" ]
    then
        obj.property port = $2
    else
        obj.property port
    fi
}

obj.name(){
    if [ "$1" == "=" ]
    then
        obj.property name = $2
    else
        obj.property name
    fi
}